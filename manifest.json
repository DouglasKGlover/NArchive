{
   "browser_action": {
      "default_icon": "icon.png"
   },
   "content_scripts": [ {
      "css": [ "main.css" ],
      "js": [ "javascript.js" ],
      "matches": [ "https://connect.gocollect.com/*" ]
   } ],
   "description": "A 'fix' for the lack of proper redirects on GoCollect's archive of NintendoAge.com",
   "manifest_version": 2,
   "name": "NArchive",
   "permissions": [ "tabs", "activeTab", "storage" ],
   "version": "1.0"
}
